# Backend environment example
# Copy to .env and fill values for your environment.

# Node environment: 'development' or 'production'
NODE_ENV=development

# HTTP port to run the API server locally
PORT=5000

# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/geo-ranking

# JWT secret for signing access tokens
JWT_SECRET=change_me

# Frontend origin allowed in development (CORS)
# Example for Vite dev server
FRONTEND_ORIGIN_DEV=http://localhost:5173
